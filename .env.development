# Development environment settings for the Frappe stack
COMPOSE_PROJECT_NAME=frape2_dev

# MariaDB
MARIADB_ROOT_PASSWORD=dev-root-password-change-me
MARIADB_ROOT_PASSWORD_SHA256=

# Frappe site
FRAPPE_SITE_NAME=builder.localhost
FRAPPE_DEFAULT_SITE=builder.localhost
FRAPPE_ADMIN_EMAIL=administrator@example.com
FRAPPE_ADMIN_PASSWORD=dev-admin-password-change-me
FRAPPE_ADMIN_PASSWORD_SHA256=

# Developer conveniences
FRAPPE_DEVELOPER_MODE=1
FRAPPE_MUTE_EMAILS=1
FRAPPE_BENCH_VERSION=version-15
YARN_NETWORK_TIMEOUT=100000

# Optional database overrides (uncomment if you need deterministic credentials)
#FRAPPE_DB_NAME=builder_localhost
#FRAPPE_DB_PASSWORD=dev-db-password-change-me
#FRAPPE_DB_PASSWORD_SHA256=

# Optional: provide a deterministic encryption key (hex string, 64 chars)
#FRAPPE_ENCRYPTION_KEY=
